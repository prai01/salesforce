<apex:page sidebar="false" standardController="Account">
    
    <apex:pageBlock title="Account Summary">
        <apex:pageBlockSection >
            Account Owner: {! Account.Owner.Name}<br/>
            Name: {! Account.Name } <br/>
            Phone: {! Account.Phone } <br/>
            Industry: {! Account.Industry } <br/>
            Revenue: {! Account.AnnualRevenue } <br/>
            
        </apex:pageBlockSection>
    </apex:pageBlock>
    <apex:iframe src="http://www.salesforce.com" scrolling="true" id="theIframe"/>
    <apex:iframe id="iSellDemoiFrame" frameborder="true" title="iSellDemo" scrolling="true" src="http://www.iselldemo.com"/>
</apex:page>